  // RestaurantRepository.java
  public interface RestaurantRepository extends JpaRepository<Restaurant, Integer> {
      // ìƒì†ë°›ì€ findAllById() ë©”ì„œë“œê°€ ë‹¤ìŒ SQLì„ ìë™ ìƒì„±:
      // SELECT * FROM restaurants WHERE pk IN (5, 1, 12, 23, 7);
