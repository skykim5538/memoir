  1.4 Entity í´ë˜ìŠ¤ì™€ DB ë§¤í•‘

  // Restaurant.java
  @Entity
  @Table(name = "restaurants")
  @Getter
  @Setter
  @NoArgsConstructor
  public class Restaurant {

      @Id
      @GeneratedValue(strategy = GenerationType.IDENTITY)
      private Integer pk;

      @Column(nullable = false)
      private String name;

      @Column(nullable = false)
      private Double lat;

      @Column(nullable = false)
      private Double lon;

      @Column(name = "exp_cost")
      private Double expCost;

      @Column(columnDefinition = "TEXT")
      private String description;

      @Column(name = "cuisine_type")
      private String cuisineType;

      @Column(name = "signature_menu")
      private String signatureMenu;

      @Column(name = "operating_hours")
      private String operatingHours;
  }

  4ë‹¨ê³„: MySQL ë°ì´í„°ë² ì´ìŠ¤ì—ì„œ ì‹¤ì œ ë°ì´í„° ì¡°íšŒ

  -- JPAê°€ ì‹¤í–‰í•˜ëŠ” ì‹¤ì œ SQL ì¿¼ë¦¬
  SELECT
      pk,
      name,
      lat,
      lon,
      exp_cost,
      description,
      cuisine_type,
      signature_menu,
      operating_hours
  FROM restaurants
  WHERE pk IN (5, 1, 12, 23, 7);

  -- ê²°ê³¼ ì˜ˆì‹œ:
  -- pk=5: "ê´‘í™”ë¬¸ í•œì •ì‹", lat=37.5665, cuisine_type="í•œì‹", ...
  -- pk=1: "ëª…ë™êµì", lat=37.5636, cuisine_type="í•œì‹", ...
  -- pk=12: "ì¢…ë¡œ ëƒ‰ë©´ì§‘", lat=37.5704, cuisine_type="í•œì‹", ...

  1.5 Entity â†’ DTO ë³€í™˜ ê³¼ì •

  // RestaurantDto.java
  public record RestaurantDto(
      Integer pk,
      String name,
      Double lat,
      Double lon,
      Double expCost,
      String description,
      String cuisineType,
      String signatureMenu,
      String operatingHours
  ) {
      // record classëŠ” ìë™ìœ¼ë¡œ ìƒì„±ì, getter, equals, hashCode, toString ì œê³µ
      // Python RestaurantDetail ìŠ¤í‚¤ë§ˆì™€ ë™ì¼í•œ êµ¬ì¡°ë¡œ ë°ì´í„° í˜¸í™˜ì„± í™•ë³´
  }

  5ë‹¨ê³„: ë°ì´í„° ë³€í™˜ ë° ê°€ê³µ

  // Serviceì˜ convertToDto ë©”ì„œë“œ
  private RestaurantDto convertToDto(Restaurant restaurant) {
      // MySQLì—ì„œ ì¡°íšŒí•œ Restaurant Entityë¥¼ DTOë¡œ ë³€í™˜
      return new RestaurantDto(
          restaurant.getPk(),           // 5
          restaurant.getName(),         // "ê´‘í™”ë¬¸ í•œì •ì‹"
          restaurant.getLat(),          // 37.5665
          restaurant.getLon(),          // 126.9780
          restaurant.getExpCost(),      // 50000.0
          restaurant.getDescription(),  // "ê²½ë³µê¶ ê·¼ì²˜ ì „í†µ í•œì •ì‹ ì „ë¬¸ì "
          restaurant.getCuisineType(),  // "í•œì‹"
          restaurant.getSignatureMenu(), // "ê¶ì¤‘ë–¡ê°ˆë¹„"
          restaurant.getOperatingHours() // "11:00-22:00"
      );
  }

  // Stream APIë¡œ ì¼ê´„ ë³€í™˜
  List<RestaurantDto> result = restaurants.stream()
      .map(this::convertToDto)  // ê° Restaurantì„ RestaurantDtoë¡œ ë³€í™˜
      .toList();

  // ê²°ê³¼:
  // [
  //   RestaurantDto(pk=5, name="ê´‘í™”ë¬¸ í•œì •ì‹", cuisineType="í•œì‹", ...),
  //   RestaurantDto(pk=1, name="ëª…ë™êµì", cuisineType="í•œì‹", ...),
  //   RestaurantDto(pk=12, name="ì¢…ë¡œ ëƒ‰ë©´ì§‘", cuisineType="í•œì‹", ...)
  // ]

