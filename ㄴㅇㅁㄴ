  3.1 Pythonì—ì„œ ë°ì´í„° êµ¬ì¡°í™”

  # Python ìŠ¤í‚¤ë§ˆ ì •ì˜ (planning.py)
  class RestaurantDetail(BaseTravelDetail):
      """
      ì‹ë‹¹ì— ëŒ€í•œ ì„¸ë¶€ ì •ë³´ ìŠ¤í‚¤ë§ˆì…ë‹ˆë‹¤.
      RDBì˜ pkì™€ ê°™ì€ ê³ ìœ  ì‹ë³„ìë¥¼ í¬í•¨í•©ë‹ˆë‹¤.
      """
      pk: int
      cuisine_type: str | None
      signature_menu: str | None
      operating_hours: str | None

  class TravelData(TypedDict):
      """
      ì—¬í–‰ ê³„íš ì—ì´ì „íŠ¸ì˜ ì „ì²´ ìƒíƒœ(State)ë¥¼ ì •ì˜í•˜ëŠ” ìŠ¤í‚¤ë§ˆì…ë‹ˆë‹¤.
      ì´ ë°ì´í„° êµ¬ì¡°ëŠ” LangGraphì˜ ë…¸ë“œë¥¼ ê±°ì¹˜ë©° ì—…ë°ì´íŠ¸ë©ë‹ˆë‹¤.
      """
      places: List[PlaceDetail]
      restaurants: List[RestaurantDetail]
      accommodations: List[AccommodationDetail]
      additional_info: List[str] | None
      worker_results: List[TravelPlan]
      plan: Optional[TravelPlan]

  1ë‹¨ê³„: Java ì‘ë‹µ ë°ì´í„°ë¥¼ Python ìŠ¤í‚¤ë§ˆë¡œ ë³€í™˜

  # Javaì—ì„œ ë°›ì€ ë°ì´í„°
  java_response = [
      {
          "pk": 5,
          "name": "ê´‘í™”ë¬¸ í•œì •ì‹",
          "lat": 37.5665,
          "lon": 126.9780,
          "expCost": 50000.0,
          "description": "ê²½ë³µê¶ ê·¼ì²˜ ì „í†µ í•œì •ì‹ ì „ë¬¸ì ",
          "cuisineType": "í•œì‹",
          "signatureMenu": "ê¶ì¤‘ë–¡ê°ˆë¹„",
          "operatingHours": "11:00-22:00"
      }
  ]

  # Python RestaurantDetail ìŠ¤í‚¤ë§ˆë¡œ ë³€í™˜
  def convert_to_restaurant_detail(java_data) -> List[RestaurantDetail]:
      restaurants = []
      for item in java_data:
          restaurant = RestaurantDetail(
              pk=item["pk"],
              name=item["name"],
              lat=item["lat"],
              lon=item["lon"],
              exp_cost=item["expCost"],
              description=item["description"],
              cuisine_type=item["cuisineType"],
              signature_menu=item["signatureMenu"],
              operating_hours=item["operatingHours"]
          )
          restaurants.append(restaurant)
      return restaurants

  # ë³€í™˜ ê²°ê³¼
  restaurants = convert_to_restaurant_detail(java_response)

  3.2 LLM í”„ë¡¬í”„íŠ¸ ìƒì„± ë° ìì—°ì–´ ì‘ë‹µ ìƒì„±

  2ë‹¨ê³„: LLM í”„ë¡¬í”„íŠ¸ì— ë°ì´í„° ì‚½ì…

  # ì‹¤ì œ ë°ì´í„°ë¥¼ í¬í•¨í•œ í”„ë¡¬í”„íŠ¸ ìƒì„± ì˜ˆì‹œ
  def create_travel_response_prompt(restaurants: List[RestaurantDetail], user_query: str):
      prompt = f"""
  ì‚¬ìš©ì ìš”ì²­: "{user_query}"

  ê²€ìƒ‰ëœ ì‹ë‹¹ ì •ë³´:
  """

      for i, restaurant in enumerate(restaurants, 1):
          prompt += f"""
  {i}. **{restaurant['name']}**
     - ìœ„ì¹˜: ìœ„ë„ {restaurant['lat']}, ê²½ë„ {restaurant['lon']}
     - ìŒì‹ ì¢…ë¥˜: {restaurant['cuisine_type']}
     - ëŒ€í‘œ ë©”ë‰´: {restaurant['signature_menu']}
     - ìš´ì˜ ì‹œê°„: {restaurant['operating_hours']}
     - ì˜ˆìƒ ë¹„ìš©: {restaurant['exp_cost']:,.0f}ì›
     - ì„¤ëª…: {restaurant['description']}
  """

      prompt += """
  ìœ„ ì •ë³´ë¥¼ ë°”íƒ•ìœ¼ë¡œ ì‚¬ìš©ìì˜ ìš”ì²­ì— ë§ëŠ” ìì—°ìŠ¤ëŸ¬ìš´ ì‹ë‹¹ ì¶”ì²œ ì‘ë‹µì„ ìƒì„±í•´ì£¼ì„¸ìš”.
  ê° ì‹ë‹¹ì˜ íŠ¹ì§•ê³¼ ì¶”ì²œ ì´ìœ ë¥¼ í¬í•¨í•˜ì—¬ ì¹œê·¼í•˜ê³  ë„ì›€ì´ ë˜ëŠ” í†¤ìœ¼ë¡œ ì‘ì„±í•´ì£¼ì„¸ìš”.
  """

      return prompt

  # ì‚¬ìš©ì ì¿¼ë¦¬: "ê²½ë³µê¶ ê·¼ì²˜ ë§›ìˆëŠ” í•œì‹ë‹¹ ì¶”ì²œí•´ì¤˜"
  user_query = "ê²½ë³µê¶ ê·¼ì²˜ ë§›ìˆëŠ” í•œì‹ë‹¹ ì¶”ì²œí•´ì¤˜"
  final_prompt = create_travel_response_prompt(restaurants, user_query)

  3ë‹¨ê³„: LLM í˜¸ì¶œ ë° ìì—°ì–´ ì‘ë‹µ ìƒì„±

